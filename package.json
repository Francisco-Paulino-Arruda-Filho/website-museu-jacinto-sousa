{
  "name": "website-museu-jacinto-sousa",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "em:start": "firebase emulators:start --import ./emulator_data",
    "em:dev": "firebase emulators:exec --import ./emulator_data  \"npm run dev\"",
    "test": "npx concurrently \"npm run em:start\" \"npm run cypress:open\""
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.18",
    "@mui/material": "^5.15.15",
    "@mui/x-date-pickers": "^7.5.0",
    "dayjs": "^1.11.11",
    "dotenv": "^16.4.5",
    "firebase": "^10.12.0",
    "mui-tel-input": "^5.1.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.3",
    "react-iframe": "^1.8.5",
    "react-router-dom": "^6.23.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-react": "^4.2.1",
    "cypress": "^13.9.0",
    "cypress-firebase": "^4.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.4.5",
    "vite": "^4.5.3"
  }
}
